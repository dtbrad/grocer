<h4 class="text-center">Admin Panel</h4>
<div class="panel panel-default">
  <% if @nick_name_requests.length > 0 %>
    <%= render partial: 'nick_name_requests', locals: { nick_name_requests: @nick_name_requests } %>
  <% else %>
    <h4 class="text-center">No unreviewed nickname requests</h4>
  <% end %>
</div>

<h4 class="text-center">Users With Baskets</h4>
<div class="panel panel-default">
  <%= render partial: 'users', locals: { users: @users_with_baskets } %>
</div>

<h4 class="text-center">Users Without Baskets</h4>
<div class="panel panel-default">
  <%= render partial: 'users', locals: { users: @users_without_baskets } %>
</div>
