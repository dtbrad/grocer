<% if !current_user %>
  <%= render 'welcome' %>
  <%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in", class: "btn btn-success btn-block" %></br>
  <%= form_tag("/sessions", method: "post", id: 'fake') do %>
    <%= hidden_field_tag(:email, "sampleuser@mail.com",) %>
    <%= link_to 'Or load sample purchase history data instead', "#", class: "btn btn-primary btn-block", :onclick => "$('#fake').submit()" %>
  <% end %>
<% end %>
